To enter shell
docker exec -it 33017bf774a3 psql -U postgres

To insert Artists 
docker cp ./csv/artists.csv 33017bf774a3:/artists.csv
docker exec -it pg-docker psql -U postgres -a music -c "\COPY artists(id, name) FROM 'artists.csv' DELIMITER ',' CSV HEADER"

To insert Albums
docker cp ./csv/albums.csv 33017bf774a3:/albums.csv
docker exec -it pg-docker psql -U postgres -a music -c "\COPY albums(id, name, artURL, artistID) FROM 'albums.csv' DELIMITER ',' CSV HEADER"

To insert songs
docker cp ./csv/songs.csv 33017bf774a3:/songs.csv
docker exec -it pg-docker psql -U postgres -a music -c "\COPY songs(id, name, posted, tag, songURL, albumId) FROM 'songs.csv' DELIMITER ',' CSV HEADER"

To insert comments
docker cp ./csv/comments.csv 33017bf774a3:/comments.csv
docker exec -it pg-docker psql -U postgres -a music -c "\COPY comments(id, username, comment, timeStamp, avatarPicURL, songId) FROM 'comments.csv' DELIMITER ',' CSV HEADER"